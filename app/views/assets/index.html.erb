
<ol class="assets">
	
<% @assets.each do |asset| %>

	<li class="thumb" id="<%= asset.id %>">
		
		<%= link_to asset do %>
			<%= asset_image_tag asset, :small %>
			<span class="name"><%= asset.name %></span>
		<% end %>
	</li>
<% end %>
</ol>


<% content_for :side do %>

<%= render 'crud' %>

<% end %>